{
  "name": "joshuaschmidt",
  "version": "1.0.0",
  "description": "personal website front-end nuxt",
  "author": "Joshua Schmidt",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "email:build": "mjml emailtemplates/*.mjml -o static/emailtemplates/",
    "email:dev": "mjml -u emailtemplates/**/*.mjml",
    "build": "yarn email:build && nuxt build",
    "build:spa": "nuxt build --spa",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate --no-lock",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .eslintignore .",
    "precommit": "yarn lint",
    "predeploy": "yarn precommit && yarn email:build && yarn generate",
    "serve": "node staticserver.js",
    "now-build": "yarn predeploy",
    "deploy:gcp": "gcloud app deploy",
    "deploy:heroku": "sh ./deployheroku.sh",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxt/typescript": "^2.6.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^1.0.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "connect": "^3.6.6",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "epic-spinners": "^1.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.4.0-beta.2",
    "nuxt": "^2.4.0",
    "prismjs": "^1.16.0",
    "serve-static": "^1.13.2",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "vanilla-lazyload": "^12.0.0",
    "vue-color": "^2.7.0",
    "vue-markdown": "^2.2.4",
    "vue-recaptcha-v3": "^1.5.0",
    "vue-select": "^3.1.0",
    "vue-toasted": "^1.1.27",
    "vuelidate": "^0.7.4",
    "vuex-persistedstate": "^2.5.4",
    "webpack-dev-middleware": "^3.6.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/node": "^12.0.8",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3"
  }
}
