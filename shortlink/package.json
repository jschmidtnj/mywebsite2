{
  "name": "joshshort",
  "version": "1.0.0",
  "description": "link shortening service",
  "author": "Joshua Schmidt",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_PORT=3001 nodemon server/index.js --watch server",
    "build": "nuxt-ts build",
    "build:spa": "nuxt-ts build --spa",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt-ts generate --no-lock",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .eslintignore .",
    "precommit": "yarn lint",
    "predeploy": "yarn precommit && yarn generate",
    "serve": "node staticserver.js",
    "now-build": "yarn predeploy",
    "deploy:gcp": "gcloud app deploy",
    "deploy:heroku": "sh ./deployheroku.sh",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxt/typescript-runtime": "^0.3.8",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^1.0.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "connect": "^3.6.6",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "epic-spinners": "^1.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.4.0-beta.2",
    "nuxt": "^2.4.0",
    "prismjs": "^1.16.0",
    "progressive-image.js": "^1.4.0",
    "serve-static": "^1.13.2",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "vue-color": "^2.7.0",
    "vue-markdown": "^2.2.4",
    "vue-recaptcha-v3": "^1.5.0",
    "vue-select": "^3.1.0",
    "vue-toasted": "^1.1.27",
    "vuelidate": "^0.7.4",
    "vuex-persistedstate": "^2.5.4",
    "webpack-dev-middleware": "^3.6.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.6",
